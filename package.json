  "main": "./build/app.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -Rf .nyc_output && rm -Rf coverage && rm -Rf build ",
    "codecov": "codecov",
    "doc": "typedoc --options typedoc.js src/index.ts",
    "watch-node": "nodemon app.js --ignore ./test/",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run lint && npm test && npm run doc",
    "pretest": "tsc --build",
    "preversion": "npm run lint && npm test",
    "test": "nyc mocha --require ts-node/register --ui mocha-typescript --require source-map-support/register 'test/**/*.spec.ts'",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w"
  },
